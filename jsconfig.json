{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "$lib": ["src/lib"],
      "$lib/*": ["src/lib/*"],
      "$docs/*": ["src/docs/*"],
      "$types": ["src/types/jsdoc.js"]
    }
  },
  "include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"],
  "extends": "./.svelte-kit/tsconfig.json"
}
