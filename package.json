{
  "name": "bubbles-ui",
  "version": "0.9.0-next.10",
  "description": "Opinionated Svelte Kit UI Components",
  "scripts": {
    "dev": "vite dev --port 3001",
    "watch": "svelte-package --watch",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
